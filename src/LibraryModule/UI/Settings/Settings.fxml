<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="SettingsPane" prefHeight="300.0" prefWidth="400.0" stylesheets="@Settings.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LibraryModule.UI.Settings.SettingsController">
   <children>
      <VBox layoutX="150.0" layoutY="200.0" prefHeight="230.0" prefWidth="400.0" AnchorPane.bottomAnchor="70.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <JFXTextField fx:id="NoOfDays" labelFloat="true" promptText="No Of Days">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="Fine" labelFloat="true" layoutX="10.0" layoutY="10.0" promptText="Fine">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="UName" labelFloat="true" layoutX="10.0" layoutY="36.0" promptText="User Name">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXPasswordField fx:id="Password" labelFloat="true" promptText="Password">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
            </JFXPasswordField>
         </children>
      </VBox>
      <Pane layoutY="230.0" prefHeight="70.0" prefWidth="400.0">
         <children>
            <JFXButton fx:id="Save" onAction="#HandleSave" prefHeight="70.0" prefWidth="200.0" text="Save Settings" />
            <JFXButton fx:id="Cancel" layoutX="200.0" onAction="#HandelCancel" prefHeight="70.0" prefWidth="200.0" text="Cancel" />
         </children>
      </Pane>
   </children>
</AnchorPane>
